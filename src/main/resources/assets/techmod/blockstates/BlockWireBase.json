{
    "multipart": [
        {   "apply": { "model": "techmod:BlockWireBase" }},
        {   "when": { "north": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "y": 180 }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "y": 90 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "y": 270 }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "x": -90 }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "techmod:BlockWireExtend", "x": 90 }
        }
    ]
}